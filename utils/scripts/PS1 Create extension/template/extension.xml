<?xml version="1.0" encoding="UTF-8"?>
<extension format="1.0">
	<extension_code>{{ extName }}</extension_code>
	<company><![CDATA[ {{ extCompany }} ]]></company>
	<author><![CDATA[ {{ extAuthor }} ]]></author>
	<label><![CDATA[ {{ extLabel }} ]]></label>
	<description><![CDATA[ {{ extDescription }} ]]></description>
	<version>{{ extVersion }}</version>
	<release_date>{{ extReleaseDate }}</release_date>
	<version_description><![CDATA[ {{ extVersionDescription }} ]]></version_description>
	<itop_version_min>{{ extVersionMin }}</itop_version_min>
	<status>stable</status>
	<mandatory>false</mandatory>
	<more_info_url>{{ extUrl }}</more_info_url>
</extension>


